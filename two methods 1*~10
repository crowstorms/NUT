package hello;
public class homework1 {
    public static void main(String[] args) {
        int n=1;
        int i=1;
    while(i<=10){
        n=n*i;
        i++;
    }
        System.out.println(n);
    }
}
package hello;
public class homework1 {
    public static void main(String[] args) {
        int n = 1;
        int i;
    for(i=1;i<=10;i++){
        n=n*i;
    }
        System.out.println(n);
    }
}
